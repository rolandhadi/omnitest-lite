Function GetMatchingText(e) ' [KEYWORD=TRUE] '@L1S2 @L2S2	Dim outValue, strTarget, inputId, regExpVal	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		outValue = stepReference.GetROProperty(myPactera_commonFunction_getObjectTextValue(stepReference))		omnilite_update_data	outValue, e("data_out")	End If	Set GetMatchingText = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function