Function SetObjectSetText(e) ' [KEYWORD=TRUE] '@L1S1	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		If IsObject(stepReference.Object) Then			If Trim(UCase(stepData)) <> "%SKIP%" Then stepReference.Object.setText stepData		Else			If Trim(UCase(stepData)) <> "%SKIP%" Then stepReference.Set stepData		End If	End If	Set SetObjectSetText = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function