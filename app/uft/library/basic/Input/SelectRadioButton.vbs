Function SelectRadioButton(e) ' [KEYWORD=TRUE] '@L1S1	Dim counterValue, tempValue	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		If Left(stepData, 1) = "#" Then			stepReference.Select stepData		ElseIf Trim(Ucase(stepData)) = "ON" Then			stepReference.Set "ON"		ElseIf Trim(Ucase(stepData)) = "OFF" Then			stepReference.Set "OFF"		Else			tempValue = Split(stepReference.GetROProperty("all items"), ";")			For counterValue = LBound(tempValue) To UBound(tempValue)				If Trim(UCase(tempValue(counterValue))) = Trim(UCase(stepData)) Then					stepReference.Select "#" & counterValue					Exit For				End If			Next		End If	End If	Set SelectRadioButton = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function