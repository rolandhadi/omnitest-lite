Function MouseRClickXY(e) ' [KEYWORD=TRUE] '@L1S1	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		temp = Split(stepData, "~!!!~")		temp(0) = trim(temp(0))		temp(1) = trim(temp(1))		If Right(temp(0),1) = "%" Then			temp(0) = Left(temp(0),Len(temp(0))-1)			temp(1) = Left(temp(1),Len(temp(1))-1)			CreateObject("Mercury.DeviceReplay").MouseClick myPactera_commonFunction_GetScreenX * temp(0) / 100, myPactera_commonFunction_GetScreenY * temp(1) /100, 2		Else			CreateObject("Mercury.DeviceReplay").MouseClick temp(0), temp(1), 2		End If	End If	Set MouseRClickXY = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function