Function MouseLClickLink(e) ' [KEYWORD=TRUE] '@L1S1 @L2S1	Dim objLink, objDesc, stepDataIndex, linkText, linkIndex	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		If IsObject(stepReference) Then			Set objDesc = Description.Create()			If Right(stepData, 1) = "]" And Instr(1, stepData, "[") <> 0 Then				stepDataIndex = Split(stepData, "[")				linkText = Trim(stepDataIndex(0))				linkIndex = CInt(Replace(stepDataIndex(1), "]", ""))			Else				linkText = stepData				linkIndex = 0			End If			objDesc("micclass").Value = "Link"			objDesc("innertext").Value = linkText			Set objLink = stepReference.ChildObjects(objDesc)			If objLink(linkIndex).Exist Then				objLink(linkIndex).highlight				CreateObject("Mercury.DeviceReplay").MouseClick objLink(linkIndex).GetROProperty("abs_x") + (objLink(linkIndex).GetROProperty("width") \ 2), objLink(linkIndex).GetROProperty("abs_y") + (objLink(linkIndex).GetROProperty("height") \ 2), 0			Else				Err.Raise 424			End If			mobileClientCaptureFromMobile = False		Else			helper_function__findSwipe stepReference, stepOptions			mobileClientInstance.Click "NATIVE", stepReference, 0, 1			mobileClientCaptureFromMobile = True		End If	End If	Set MouseLClickLink = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function