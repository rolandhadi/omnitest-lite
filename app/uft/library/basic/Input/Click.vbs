Function Click(e) ' [KEYWORD=TRUE] '@L1S1 @L2S1	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		If s("mobile") <> 1 Then			Set stepReference = s("obj")				stepReference.Click			mobileClientCaptureFromMobile = False		Else			stepReference = s("obj")				helper_function__findSwipe stepReference, stepOptions			mobileClientInstance.Click "NATIVE", stepReference, 0, 1			MOBILE_LAST_OBJECT_CLICKED_01 = stepReference			mobileClientCaptureFromMobile = True		End If	End If	Set Click = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function