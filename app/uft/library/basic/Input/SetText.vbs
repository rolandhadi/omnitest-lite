Function SetText(e) ' [KEYWORD=TRUE] '@L1S1 @L2S1	Dim objWindow	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		stepData = e("data_in") 		If Trim(e("data_out")) <> "" Then 			COMMON_GLOBAL_VARIABLE_01 = stepData			omnilite_update_data	stepData, e("data_out")		End If		If s("mobile") <> 1 Then			Set stepReference = s("obj")				Select Case stepReference.GetTOProperty("micclass")			Case "JavaObject"				If Trim(stepData) <> "" Then					If Trim(UCase(strSetValue)) <> "%SKIP%" Then stepReference.Object.SetText stepData				End If			Case "WebFile"				stepReference.Click				If Err.Number = 0 Then					If Not Browser("CreationTime:=" & myPactera_commonFunction_getActiveBrowserIndex).Dialog("nativeclass:=#32770", "text:=.*File.*").WinEdit("nativeclass:=Edit", "attached text:=File &name:").Exist(1) Then						If Trim(UCase(stepData)) <> "%SKIP%" Then							stepReference.Set ""							stepReference.SetFocus							stepReference.Set stepData						End If					End If					If Trim(stepReference.GetROProperty("value")) = "" Then						Wait (1)						Set objWindow = Browser("CreationTime:=" & myPactera_commonFunction_getActiveBrowserIndex).Dialog("nativeclass:=#32770", "text:=.*File.*")						If objWindow Is Nothing Then							If Trim(UCase(stepData)) <> "%SKIP%" Then								stepReference.Set ""								stepReference.SetFocus								stepReference.Set stepData							End If						Else							If Not objWindow.WinEdit("nativeclass:=Edit", "attached text:=File &name:").Exist(1) Then stepReference.Click stepReference.GetROProperty("width") - 5, stepReference.GetROProperty("height") - 5							If Not objWindow.WinEdit("nativeclass:=Edit", "attached text:=File &name:").Exist(1) Then stepReference.FireEvent "ondblclick"							objWindow.WinEdit("nativeclass:=Edit", "attached text:=File &name:").Set stepData							objWindow.WinButton("nativeclass:=Button", "text:=&Open").Click						End If					End If				End If			Case Else				If Trim(UCase(stepData)) <> "%SKIP%" Then stepReference.Set stepData			End Select			mobileClientCaptureFromMobile = False		Else			If Trim(UCase(stepData)) <> "%SKIP%" And Instr(1, stepData, "MATCH[") = 0 Then				stepReference = s("obj")				helper_function__findSwipe stepReference, stepOptions				mobileClientInstance.ElementSendText "NATIVE", stepReference, 0, stepData				mobileClientInstance.CloseKeyboard				mobileClientCaptureFromMobile = True			End If		End If	End If	Set SetText = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function