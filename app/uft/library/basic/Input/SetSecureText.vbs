Function SetSecureText(e) ' [KEYWORD=TRUE] '@L1S1	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		If Trim(UCase(stepData)) <> "%SKIP%" Then stepReference.SetSecure stepData	End If	Set SetSecureText = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function