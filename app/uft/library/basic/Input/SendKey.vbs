Function SendKey(e) ' [KEYWORD=TRUE] '@L1S1	Dim cancelButton, wsShell, deviceReplay	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		If Trim(e("data_out")) <> "" Then 			COMMON_GLOBAL_VARIABLE_01 = stepData			omnilite_update_data	stepData, e("data_out")		End If		If IsObject(stepReference) Then			If Trim(UCase(stepData)) <> "%SKIP%" Then				stepReference.Highlight				On Error Resume Next				Set deviceReplay = CreateObject("Mercury.DeviceReplay")				deviceReplay.MouseClick stepReference.GetROProperty("abs_x") + (stepReference.GetROProperty("width") \ 2), stepReference.GetROProperty("abs_y") + (stepReference.GetROProperty("height") \ 2), 0				Wait(1)				stepReference.Set ""				stepReference.Object.setText ""				stepReference.Click				deviceReplay.MouseClick stepReference.GetROProperty("abs_x") + (stepReference.GetROProperty("width") \ 100), stepReference.GetROProperty("abs_y") + (stepReference.GetROProperty("height") \ 2), 0				Wait(1)				Err.Clear: On Error GoTo 0				Set wsShell = CreateObject("WScript.Shell")				wsShell.SendKeys "^a"				Wait(1)				wsShell.SendKeys "{BACKSPACE}"				Wait(1)				wsShell.SendKeys stepData			End If		Else			If Trim(UCase(stepData)) = "{SEARCH}" Then				stepReference = s("obj")				helper_function__findSwipe stepReference, stepOptions				mobileClientInstance.Click "NATIVE", stepReference, 0, 1				Wait(3)				If mobileClientInstance.P2cx(100)  >= mobileClientInstance.P2cy(100) Then					If Instr(mobileClientInstance.GetConnectedDevices(), "sonyExperia") > 0 Then						mobileClientInstance.ClickCoordinate mobileClientInstance.P2cx(92), mobileClientInstance.P2cy(66), 1					Else						mobileClientInstance.ClickCoordinate mobileClientInstance.P2cx(98), mobileClientInstance.P2cy(75), 1					End If				Else					mobileClientInstance.ClickCoordinate mobileClientInstance.P2cx(98), mobileClientInstance.P2cy(98), 1				End If				mobileClientInstance.CloseKeyboard				mobileClientCaptureFromMobile = True			ElseIf Trim(UCase(stepData)) = "{ENTER}"  Then				helper_function__findSwipe stepReference, stepOptions				mobileClientInstance.Click "NATIVE", stepReference, 0, 1				mobileClientInstance.SendText stepData				mobileClientInstance.CloseKeyboard				mobileClientCaptureFromMobile = True			ElseIf Trim(UCase(stepData)) <> "%SKIP%" And Instr(1, stepData, "MATCH[") = 0 Then				helper_function__findSwipe stepReference, stepOptions				mobileClientInstance.Click "NATIVE", stepReference, 0, 1				Select Case CURRENT_DEVICE_OS				Case "ios_app"					cancelButton = I3MOBILE_IOS_CANCEL_SEARCH_01				Case "adb"					cancelButton = I3MOBILE_ADB_CANCEL_SEARCH_01				End Select				If mobileClientInstance.IsElementFound("NATIVE", cancelButton, 0) = "True" Then					mobileClientInstance.Click "NATIVE", cancelButton, 0, 1					mobileClientInstance.Click "NATIVE", stepReference, 0, 1				End If				mobileClientInstance.SendText stepData				mobileClientInstance.CloseKeyboard				mobileClientCaptureFromMobile = True			End If		End If	End If	Set SendKey = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function