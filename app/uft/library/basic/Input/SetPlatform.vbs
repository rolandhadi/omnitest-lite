Function SetPlatform(e) ' [KEYWORD=TRUE] '@L4	Dim tmpOutput	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		Select Case UCase(stepData)		Case "WEB"			SET_PLATFORM "WEB"		Case "MOBILE"			SET_PLATFORM CURRENT_DEVICE_OS		Case Else			SET_PLATFORM "WEB"		End Select	End If	Set SetPlatform = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function