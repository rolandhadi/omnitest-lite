Function SetObjectText(e) ' [KEYWORD=TRUE] '@L1S1	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		If Trim(e("data_out")) <> "" Then 			COMMON_GLOBAL_VARIABLE_01 = stepData			omnilite_update_data	stepData, e("data_out")		End If		If IsObject(stepReference.Object) Then			If Trim(UCase(stepData)) <> "%SKIP%" Then stepReference.Object.value = stepData		Else			If Trim(UCase(stepData)) <> "%SKIP%" Then stepReference.Set stepData		End If	End If	Set SetObjectText = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function