Function MouseDoubleLClick(e) ' [KEYWORD=TRUE] '@L1S1	Dim iCounter	Err.Clear: On Error GoTo 0	On Error Resume Next	stepData = CInt(myPactera_commonFunction_convertToText(stepData))	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepReference.Highlight		CreateObject("Mercury.DeviceReplay").MouseMove stepReference.GetROProperty("abs_x") + (stepReference.GetROProperty("width") \ 2), stepReference.GetROProperty("abs_y") + (stepReference.GetROProperty("height") \ 2)		CreateObject("Mercury.DeviceReplay").MouseDblClick stepReference.GetROProperty("abs_x") + (stepReference.GetROProperty("width") \ 2), stepReference.GetROProperty("abs_y") + (stepReference.GetROProperty("height") \ 2), 0	End If	Set MouseDoubleLClick = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function