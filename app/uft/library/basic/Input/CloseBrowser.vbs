Function CloseBrowser(e) ' [KEYWORD=TRUE] '@L1S1	Err.Clear: On Error GoTo 0	On Error Resume Next	currentRunningPlatform = "WEB"	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in")  'Added by Allen 		If Browser("index:=0").Exist Then			print "00: clearing cookies and cache before closing browser"		End If		If IsEmpty(stepReference) Then			If stepData = "" Then				Browser("CreationTime:=" & myPactera_commonFunction_getActiveBrowserIndex).Highlight				print "01: clearing cookies and cache before closing browser"				Browser("CreationTime:=" & myPactera_commonFunction_getActiveBrowserIndex).Close			Else				If CInt(myPactera_commonFunction_getActiveBrowserIndex) >= CInt(stepData) - 1 Then					Browser("CreationTime:=" & CInt(stepData) - 1).Highlight					Browser("CreationTime:=" & CInt(stepData) - 1).Close				End If			End If		Else			stepReference.Highlight			stepReference.Close		End If	End If	Set CloseBrowser = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function