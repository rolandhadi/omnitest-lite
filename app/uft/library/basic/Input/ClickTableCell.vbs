Function ClickTableCell(e) ' [KEYWORD=TRUE] '@L3S1	Dim loadedTable, checkTable, verifyTableResult, LastCheckPointMessage, tmpValue, tmpObjectType	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in")		stepReference.Highlight		Set loadedTable = myPactera_commonFunction_extractObjectTable(stepReference)		Set checkTable = myPactera_commonFunction_loadTableData(myPactera.curTestStep, myPactera, stepData)		Set verifyTableResult = myPactera_commonFunction_compareTables(loadedTable, checkTable, "CLICK")		If verifyTableResult("Status") = stPass Then			Select Case stepReference.GetTOProperty("micclass")			Case "WebTable"				tmpValue = Chr(34) & verifyTableResult("SelectedRow") & Chr(34) & "," & Chr(34) & verifyTableResult("SelectedColumn") & Chr(34) & "," & Chr(34) & verifyTableResult("SelectedColumn") & Chr(34) & "," & Chr(34) & "WebElement" & Chr(34) & "," & Chr(34) & "0" & Chr(34) & "," & Chr(34) & "" & Chr(34)			Case "JavaTable"				tmpValue = Chr(34) & verifyTableResult("SelectedRow") - 1 & Chr(34) & "," & Chr(34) & verifyTableResult("SelectedColumn") - 1 & Chr(34) & "," & Chr(34) & verifyTableResult("SelectedColumn") & Chr(34) & "," & Chr(34) & "" & Chr(34)			End Select			TableClickByRef stepReference, tmpValue, "", "", ""		Else			Err.Raise 513		End If		LastCheckPointMessage = verifyTableResult("Remarks")	End If	Set ClickTableCell = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function