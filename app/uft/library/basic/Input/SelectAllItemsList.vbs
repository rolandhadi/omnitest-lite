Function SelectAllItemsList(e) ' [KEYWORD=TRUE] '@L1S1	Dim iCounter	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		For iCounter = 0 To stepReference.GetROProperty("items count") - 1			stepReference.ExtendSelect "#" & iCounter		Next	End If	Set SelectAllItemsList = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function