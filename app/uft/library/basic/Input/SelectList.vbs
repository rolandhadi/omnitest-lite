Function SelectList(e) ' [KEYWORD=TRUE] '@L1S1	Dim allList, totalListCount, tmpIndex, fromIndex, selectedIndex	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		If Trim(e("data_out")) <> "" Then 			COMMON_GLOBAL_VARIABLE_01 = stepData			omnilite_update_data	stepData, e("data_out")		End If		If Trim(UCase(stepData)) <> "%SKIP%" Then			If InStr(1, stepData, "RANDOM_FROM_LIST") <> 0 Then				allList = stepReference.GetROProperty("all items")				allList = Split(allList, ";")				totalListCount = UBound(allList) - 1				fromIndex = 0				If InStr(1, stepData, "[") <> 0  And InStr(1, stepData, "]") <> 0 Then					tmpIndex = Split(stepData, "[")					fromIndex = CInt(Replace(tmpIndex(1), "]", "")) - 1				End If				Randomize				selectedIndex = Cint((totalListCount - fromIndex + 1) * Rnd + fromIndex)				stepReference.Select "#" & selectedIndex			Else				stepReference.Select stepData			End If		End If	End If	Set SelectList = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function