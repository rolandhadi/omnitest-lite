Function LaunchBrowser(e) ' [KEYWORD=TRUE] '@L1S1	Err.Clear: On Error GoTo 0	On Error Resume Next	currentRunningPlatform = "WEB"	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in")		systemutil.CloseProcessByName ("iexplore.exe"): 		systemutil.CloseProcessByName ("chrome.exe"): 		systemutil.CloseProcessByName ("firefox.exe")		If InStr(1, stepData, ",") > 0 Then			launch_data = Split(stepData, ",")			SystemUtil.run launch_data(0), launch_data(1), "", "", 3		Else			SystemUtil.run "iexplore.exe", stepData, "", "", 3		End If		WebUtil.DeleteCookies		Set wShell = CreateObject("WScript.Shell")		wShell.SendKeys "^0"	End If	Set LaunchBrowser = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function