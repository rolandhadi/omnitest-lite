Function VerifyLinkNotExist(e) ' [KEYWORD=TRUE] '@L1S1 @L2S1	Dim objLink, objDesc	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		If s("mobile") <> 1 Then			Set stepReference = s("obj")				Set objDesc = Description.Create()			objDesc("micclass").Value = "Link"			objDesc("innertext").Value = stepData			If Browser("CreationTime:=" & myPactera_commonFunction_getActiveBrowserIndex).Exist Then				Browser("CreationTime:=" & myPactera_commonFunction_getActiveBrowserIndex).Sync			End If			Set objLink = stepReference.ChildObjects(objDesc)			If objLink.Count = 0 Then				lastCheckPointStatus = stPass			Else				LastCheckPointMessage = "Link does exist"				objLink(0).Highlight				Err.Raise 514			End If			mobileClientCaptureFromMobile = False		Else			stepReference = s("obj")			helper_function__findSwipe stepReference, stepOptions			mobileClientInstance.Click "NATIVE", stepReference, 0, 1			mobileClientCaptureFromMobile = True		End If	End If	Set VerifyLinkNotExist = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function