Function VerifyObjectExist(e) ' [KEYWORD=TRUE] '@L1S3 @L2S3	Dim tmpValues, tmpValue, lastCheckPointStatus, LastCheckPointMessage	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		If s("mobile") <> 1 Then			Set stepReference = s("obj")				lastCheckPointStatus = "Pass"			If lastCheckPointStatus = stFail Then				LastCheckPointMessage = "Object does not exist"				Err.Raise 513			Else				lastCheckPointStatus = stPass				stepReference.Highlight			End If			mobileClientCaptureFromMobile = False		Else			stepReference = s("obj")			helper_function__findSwipe stepReference, stepOptions			tmpValue = mobileClientInstance.IsElementFound("NATIVE", stepReference, 0)			Set lastCheckPointStatus = myPactera_commonFunction_checkTest("EQUAL", CStr(tmpValue), "True")			If lastCheckPointStatus("Status") = stFail Then				LastCheckPointMessage = "Object does not exist"				Err.Raise 513			Else				LastCheckPointMessage = "Object does exist"			End If			mobileClientCaptureFromMobile = True		End If	End If	Set VerifyObjectExist = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function