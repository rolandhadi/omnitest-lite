Function RunExternalResource(e) ' [KEYWORD=TRUE] '@L4	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		If InStr(1, stepData, ".vbs") <> 0 Then			ExecuteFile stepData		Else			Execute stepData		End If	End If	Set RunExternalResource = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function