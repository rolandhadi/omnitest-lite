Function UpdateCSVFile(e) ' [KEYWORD=TRUE] '@L4	Dim strCSVFile, strColumn, intRow, strValue, tmpArray	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		tmpValues = Split(stepData, "~!!!~")		strCSVFile = myPactera_commonFunction_fixFuntionReferenceParameters(tmpValues(0))		strColumn = myPactera_commonFunction_fixFuntionReferenceParameters(tmpValues(1))		intRow = myPactera_commonFunction_fixFuntionReferenceParameters(tmpValues(2))		strValue = myPactera_commonFunction_fixFuntionReferenceParameters(tmpValues(3))		tmpArray = myPactera.filesAndFolders.loadCSV2Array(strCSVFile)		tmpArray = myPactera.filesAndFolders.updateArrayCSVContent(tmpArray, strColumn, intRow, strValue)		myPactera.filesAndFolders.saveArray2CSV tmpArray, strCSVFile	End If	Set UpdateCSVFile = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function