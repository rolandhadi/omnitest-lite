Function DuplicateFile(e) ' [KEYWORD=TRUE] '@L4	Dim tmpValues, strActualFilePath, strDuplicateFilePath, strDuplicateFile	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		tmpValues = Split(stepData, "~!!!~")		strActualFilePath = myPactera_commonFunction_fixFuntionReferenceParameters(tmpValues(0))		strDuplicateFilePath = myPactera_commonFunction_fixFuntionReferenceParameters(tmpValues(1))		Set objFSO = CreateObject("Scripting.FileSystemObject")		If objFSO.FileExists(strActualFilePath) Then			objFSO.CopyFile strActualFilePath, strDuplicateFilePath, False			strDuplicateFile = strDuplicateFilePath			If InStrRev(strDuplicateFilePath,"\") <> 0 Then				strDuplicateFile = Trim(Mid(strDuplicateFilePath, InStrRev(strDuplicateFilePath,"\")+ 1))			End If			COMMON_GLOBAL_VARIABLE_01 = strDuplicateFile			omnilite_update_data	strDuplicateFile, e("data_out")		End If	End If	Set DuplicateFile = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function