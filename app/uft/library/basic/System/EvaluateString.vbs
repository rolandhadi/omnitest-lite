Function EvaluateString(e) ' [KEYWORD=TRUE] '@L4	Dim tmpOutput	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		tmpOutput = Eval(stepData)		tmpOutput = performTextOptions(tmpOutput, stepOptions)		COMMON_GLOBAL_VARIABLE_01 = CStr(tmpOutput)		If InStr(1, GetValue("", "VAR[COMMON_GLOBAL_VARIABLE_01]", stepOutput, "", "N"),stFail & "|") <> 0 Then Err.Raise 513	End If	Set EvaluateString = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function