Function VerifyProperty(e) ' [KEYWORD=TRUE] '@L1S3 @L2S3	Dim tmpValues, tmpValue, strPropertyToCheck, strOperation, strCheckValue, lastCheckPointStatus, LastCheckPointMessage	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		tmpValues = Split(stepData, "~!!!~")		strPropertyToCheck = myPactera_commonFunction_fixFuntionReferenceParameters(tmpValues(0))		strOperation = myPactera_commonFunction_fixFuntionReferenceParameters(tmpValues(1))		strCheckValue = myPactera_commonFunction_fixFuntionReferenceParameters(tmpValues(2))		If Trim(UCase(strPropertyToCheck)) = "EXIST" Then			If stepReference.Exist(1) And Trim(UCase(strCheckValue)) = "TRUE" Then				LastCheckPointMessage = "Expected: EQUAL TO (" & "Exist" & ") Actual: (" & "Exist" & ")"			ElseIf stepReference.Exist(1) And Trim(UCase(strCheckValue)) <> "TRUE" Then				Err.Raise 513				LastCheckPointMessage = "Expected: EQUAL TO (" & "Exist" & ") Actual: (" & "Not Exist" & ")"			ElseIf Not stepReference.Exist(1) And Trim(UCase(strCheckValue)) = "TRUE" Then				Err.Raise 513				LastCheckPointMessage = "Expected: EQUAL TO (" & "Not Exist" & ") Actual: (" & "Exist" & ")"			ElseIf Not stepReference.Exist(1) And Trim(UCase(strCheckValue)) <> "TRUE" Then				LastCheckPointMessage = "Expected: EQUAL TO (" & "Not Exist" & ") Actual: (" & "Not Exist" & ")"			End If		Else			If stepReference.Exist(10) Then				stepReference.Highlight				tmpValue = stepReference.GetROProperty(strPropertyToCheck)				Set lastCheckPointStatus = myPactera_commonFunction_checkTest(strOperation, tmpValue, strCheckValue)				If lastCheckPointStatus("Status") = stFail Then					Err.Raise 513				End If				LastCheckPointMessage = lastCheckPointStatus("Remarks")			Else				If InStr(1, UCase(Trim(strOptions)), "EXIST", vbTextCompare) <> 0 Then					LastCheckPointMessage = "VerifyProperty skipped"				Else					Err.Raise 513					LastCheckPointMessage = "Object is not available in the screen"				End If			End If		End If	End If	Set VerifyProperty = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function