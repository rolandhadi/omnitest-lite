Function VerifyTextContain(e) ' [KEYWORD=TRUE] '@L1S3 @L2S3	Dim tmpValues, tmpValue, lastCheckPointStatus, LastCheckPointMessage, statCompare_01, statCompare_02, statCompare_result	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		stepData = e("data_in") 		If s("mobile") <> 1 Then			Set stepReference = s("obj")				If Not stepReference.Exist Then				Err.Raise 513				LastCheckPointMessage = "Object does not exist"			Else				stepReference.Highlight				tmpValue = stepReference.GetROProperty(myPactera_commonFunction_getObjectTextValue(stepReference))				Set lastCheckPointStatus = myPactera_commonFunction_checkTest("CONTAIN", tmpValue, stepData)				statCompare_01 = lastCheckPointStatus("Status")				Set lastCheckPointStatus = myPactera_commonFunction_checkTest("CONTAIN", stepData, tmpValue)				statCompare_02 = lastCheckPointStatus("Status")				If statCompare_01 = stPass Or statCompare_02 = stPass Then					statCompare_result = stPass				Else					statCompare_result = stFail				End If				If statCompare_result = stFail Then					Err.Raise 513				End If				LastCheckPointMessage = lastCheckPointStatus("Remarks")			End If			mobileClientCaptureFromMobile = False		Else			stepReference = s("obj")			helper_function__findSwipe stepReference, stepOptions			tmpValue = mobileClientInstance.ElementGetProperty("NATIVE", stepReference, 0, "text")			Set lastCheckPointStatus = myPactera_commonFunction_checkTest("CONTAIN", tmpValue, stepData)			If lastCheckPointStatus("Status") = stFail Then				Err.Raise 513			End If			LastCheckPointMessage = lastCheckPointStatus("Remarks")			mobileClientCaptureFromMobile = True		End If			End If	Set VerifyTextContain = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function