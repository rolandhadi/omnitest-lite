Function VerifyTable(e) ' [KEYWORD=TRUE] '@L3S3	Dim loadedTable, checkTable, verifyTableResult, LastCheckPointMessage	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		stepReference.Highlight		Set loadedTable = myPactera_commonFunction_extractObjectTable(stepReference)		Set checkTable = myPactera_commonFunction_loadTableData(myPactera.curTestStep, myPactera, stepData)		Set verifyTableResult = myPactera_commonFunction_compareTables(loadedTable, checkTable, "VERIFY")		If verifyTableResult("Status") = stFail Then			Err.Raise 513		End If		LastCheckPointMessage = verifyTableResult("Remarks")	End If	Set VerifyTable = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function