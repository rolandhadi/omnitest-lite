Function VerifyValue(e) ' [KEYWORD=TRUE] '@L1S3 @L2S3	Dim tmpValues, strValueToCheck, strCheckValue, strOperation, lastCheckPointStatus, LastCheckPointMessage	Err.Clear: On Error GoTo 0	On Error Resume Next	Set s = Evaluate(e)	If s("run") = 1 Then		Set stepReference = s("obj")		stepData = e("data_in") 		tmpValues = Split(stepData, "~!!!~")		strValueToCheck = myPactera_commonFunction_fixFuntionReferenceParameters(tmpValues(0))		strCheckValue = myPactera_commonFunction_fixFuntionReferenceParameters(tmpValues(1))		strOperation = myPactera_commonFunction_fixFuntionReferenceParameters(tmpValues(2))		Set lastCheckPointStatus = myPactera_commonFunction_checkTest(strOperation, strValueToCheck, strCheckValue)		If lastCheckPointStatus("Status") = stFail Then			Err.Raise 513		End If		LastCheckPointMessage = lastCheckPointStatus("Remarks")	End If	Set VerifyValue = EvaluateExecution(Err, "")	Err.Clear: On Error GoTo 0End Function